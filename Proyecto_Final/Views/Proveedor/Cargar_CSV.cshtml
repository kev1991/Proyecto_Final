
@{
    ViewBag.Title = "Cargar_CSV";
}

<h2> Cargar CSV</h2>

@using (Html.BeginForm("uploadcsv", "proveedor", FormMethod.Post, new { enctype = "multipsart/form-date" }))
{
    <div class="form-group">
        <label class="control-label col-md-2">Cargar CSV</label>
        <div class="col-md-10">
            <input type="file" name="fileForm" class="form-control" required />
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Enviar" />
        </div>
    </div>
}

