
@{
    ViewBag.Title = "Carga_CSV_Cliente";
}

<h2> Carga CSV de  Clientes </h2>


@using (Html.BeginForm("upload_csv_cliente", "Cliente", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form-group">
        <label class="control-label col-md-2">Cargar CSV</label>
        <div class="col-md-10">
            <input type="file" name="file_Form_Cliente" class="form-control" required />
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Enviar" />
        </div>
    </div>
}
